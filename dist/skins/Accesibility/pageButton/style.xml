<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Style>
	<Container x="0%" y="0%" name="btnContainer">
		
		<Container x="0%" y="0%" name="btnContent" containerType="button">
			

			<State buttonDisplay="imageOnly">
				<Container x="0%" y="0%" name="mcHolder" />
			</State>

			<State buttonDisplay="imageAndText">
				<Container x="0%" y="0%" name="mcHolder" />
				<Text content="NOS MENUS" name="txtTitle" messageID="" textFormat="Productname" x="0%" y="11.25%" width="25.74%" height="2.7%" wordWrap="true" isUppercase="true"/>
			</State>			
			

			<State buttonDisplay="textOnly">
				<Container x="0%" y="0%" name="mcBg">
					<Shape style="BackBackground" x="0%" y="0%"  width="36.66%" height="20.62%"/>
				</Container>
				<Text content="NOS MENUS" name="txtTitle" messageID="" textFormat="Productname" x="0%" y="11.25%" width="25.74%" height="2.7%" wordWrap="true" isUppercase="true"/>
			</State>

			<State hasPrice="1">
				<Container x="12.77%" y="2.08%" name="mcPrice">
					<Shape style="BlackPriceBackground" x="0%" y="0%"  width="9.25%" height="2%"/>
					<Text content="0.00" name="strLabel" messageID="" textFormat="FirstScreenViewTitle" x="0%" y="0%" width="9.25%" height="2%" autoSize="true" />
				</Container>
			</State>
			
			<Container x="3.7%" y="0.62%" name="indexPageBtn">
				<Shape style="PlusTardCircle" x="-1" y="-1" width="3.7%" height="2.08%" />
				<Text content="1" name="indexTxt" messageID="" textFormat="Productname" x="-2" y="0%" autoSize="true" width="2.87%" height="1.61%"/>
			</Container>
			
			<Container x="0%" y="3.64%" name="mcUnavailable">
				<Shape style="UnavailabBackground" x="0%" y="0%" width="17.03%" height="1.66%"/>
				<Text content="" name="unavailableTxt" messageID="2017012703" textFormat="ProductPrice" x="0%" y="0%" width="17.03%" height="1.66%" autoSize="true"/>
			</Container>
			
		</Container>
		
		<State hasInfo="1">
			<Container x="0%" y="0.72%" containerType="button" name="btnInfo">
				<Shape style="InfoIconBg" x="0%" y="0%"  width="2.59%" height="1.45%"/>
				<Text content="i" name="" messageID="" textFormat="InfoIconText" x="-5" y="-5" width="3.33%" height="1.87%" autoSize="true" />
			</Container>
		</State>
		
		<State hasQuantity="0.05%">
			<Container x="0%" y="0.72%" name="spQuantityControls">
				<Container x="0%" y="0%" name="btnQuantityPlus">
					<Shape style="InOutCircle" x="0%" y="0%"  width="2.59%" height="1.45%"/>
					<Text content="" name="txtQuantity" messageID="" textFormat="InfoIconText" x="-5" y="-4" width="3.33%" height="1.87%" autoSize="true" />
				</Container>
				<Container x="3.7%" y="0%" containerType="button" name="btnQuantityMinus">
					<Shape style="InfoIconBg" x="0%" y="0%" width="2.59%" height="1.45%"/>
					<Text content="-" name="" messageID="" textFormat="InfoIconText" x="-5" y="-6" width="3.33%" height="1.87%" autoSize="true" />
				</Container>
			</Container>
		</State>
		
		
	</Container>
</Style>